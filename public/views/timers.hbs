<section>
    <h4>Timer Dashboard</h4>
   {{> _createTimerForm}}
</section>

<section>
    <h5>My Timers</h5>
    <div class="card">
        <div class="card-content">
            {{#each timers}}
                <div class="collection">
                    <a href="/timers/{{this._id}}" class="collection-item">{{this.timerName}}</a>
                </div>
            {{/each}}

        </div>

        
   </div>
</section>
